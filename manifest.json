{
	"name": "OneDrive Chromebook Filesystem",
	"version": "1.0",
	"default_locale": "en_us",
	"permissions": [
		"fileSystemProvider",
		"identity",
		"storage",
		"notifications",
		"https://api.onedrive.com/",
		"https://*.files.1drv.com/",
		"https://*.up.1drv.com/"
	],
	"file_system_provider_capabilities": {
		"configurable": true,
		"watchable": true,
		"multiple_mounts": false,
		"source": "network"
	}
}